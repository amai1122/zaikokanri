<?xml version="1.0" encoding="UTF-8"?>


<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
    http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    id="WebApp_ID" version="2.5">

  <display-name>在庫管理システム</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>ItemInitialize</servlet-name>
    <servlet-class>商品マスタ.ItemInitialize</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ItemInitialize</servlet-name>
    <url-pattern>/servlet/商品マスタ.ItemInitialize</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>InsertItems</servlet-name>
    <servlet-class>商品マスタ.InsertItems</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>InsertItems</servlet-name>
    <url-pattern>/servlet/商品マスタ.InsertItems</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>NewItems</servlet-name>
    <servlet-class>商品マスタ.NewItems</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>NewItems</servlet-name>
    <url-pattern>/servlet/商品マスタ.NewItems</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>UpdateItems</servlet-name>
    <servlet-class>商品マスタ.UpdateItems</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>UpdateItems</servlet-name>
    <url-pattern>/servlet/商品マスタ.UpdateItems</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>DeleteItems</servlet-name>
    <servlet-class>商品マスタ.NewItems</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>DeleteItems</servlet-name>
    <url-pattern>/servlet/商品マスタ.DeleteItems</url-pattern>
  </servlet-mapping>

</web-app>